{
  "name": "hyunmoahn-github-io",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --locale ko",
    "start:en": "docusaurus start --locale en",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "GIT_USER=hyunmoahn DEPLOYMENT_BRANCH=gh-pages docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc --project ./tsconfig.json",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "fix": "prettier src --write",
    "precommit": "lint-staged && yarn typecheck",
    "postinstall": "yarn husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn fix"
    ]
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.20.0",
    "@docusaurus/core": "3.9.2",
    "@docusaurus/plugin-pwa": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@docusaurus/theme-mermaid": "3.9.2",
    "@mdx-js/react": "^3.1.1",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/themes": "^3.3.0",
    "@svgr/webpack": "^8.1.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "docusaurus-plugin-sass": "^0.2.6",
    "file-loader": "^6.2.0",
    "lucide-react": "^0.564.0",
    "prism-react-renderer": "^2.4.1",
    "raw-loader": "^4.0.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sass": "^1.97.3",
    "styled-components": "^6.3.9",
    "url-loader": "^4.1.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@argos-ci/cli": "^4.1.0",
    "@argos-ci/playwright": "^6.4.1",
    "@docusaurus/module-type-aliases": "3.9.2",
    "@playwright/test": "^1.58.2",
    "@tsconfig/docusaurus": "^2.0.9",
    "@types/react": "18.2.20",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "cheerio": "^1.2.0",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "stylelint": "^17.3.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "5.9.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.11.0"
}
